{
  "name": "swipe-list-sdk",
  "version": "0.1.5",
  "description": "Shared SwipeList React SDK",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.mjs"
  },
  "keywords": [
    "react",
    "swipe",
    "sdk",
    "tinder-style",
    "watchlist",
    "react-spring",
    "use-gesture"
  ],
  "author": "devjohxylon",
  "license": "MIT",
  "peerDependencies": {
    "@use-gesture/react": "^10.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "react-spring": "^9.0.0 || ^10.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@storybook/cli": "^9.0.15",
    "@storybook/react": "^9.0.15",
    "rollup": "^4.44.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3"
  }
}
